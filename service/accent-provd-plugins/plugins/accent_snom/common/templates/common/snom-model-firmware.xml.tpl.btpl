<?xml version="1.0" encoding="UTF-8" ?>
<firmware-settings>
  <firmware>{{ XX_server_url }}/firmware/#FW_FILENAME#?mac={mac}</firmware>
  {% if XX_uxm_firmware == 'uxm' -%}
  <firmware_uxm>{{ XX_server_url }}/firmware/snomUXM-2.1.1.bin?mac={mac}</firmware_uxm>
  {% elif XX_uxm_firmware == 'uxmc' -%}
  <firmware_uxm>{{ XX_server_url }}/firmware/snomD7C-1.1.1-r.bin?mac={mac}</firmware_uxm>
  {% endif %}
</firmware-settings>
