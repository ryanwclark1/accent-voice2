[twilio-gateway-tpl](!)
{% for option_name, option_value in endpoint.all_options(exclude_options) -%}
{% if option_name == 'allow' -%}
disallow = all
{% endif -%}
{{ option_name }} = {{ option_value }}
{% endfor -%}
type = peer
description = Autogenerated

{% for gateway in gateways %}
[twilio-gateway{{ "%02d"|format(loop.index) }}](twilio-gateway-tpl)
host = {{ gateway.ip_address }}
{% endfor %}
