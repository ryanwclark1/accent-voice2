#!/bin/sh
# Copyright 2023 Accent Communications

case "$1" in
    build)
        echo "Building..."
        ;;
    package)
        echo "Packaging..."
        ;;
    install)
        echo "Installing..."
        ;;
    uninstall)
        echo "Removing..."
        ;;
    *)
        echo "$0 called with unknown argument '$1'" >&2
        exit 1
        ;;
esac
